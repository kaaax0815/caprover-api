{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "name": "Launch",
      "request": "launch",
      "program": "${workspaceFolder}/src/test.ts",
      "outputCapture": "std",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": ["run", "--inspect=127.0.0.1:9229", "--allow-all"],
      "env": {
        "DEBUG": "*"
      },
      "attachSimplePort": 9229
    }
  ]
}
